{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "parserOptions": {
        "ecmaVersion": 13,
        "sourceType": "module"
    },
    "globals": {
        "MyGlobal": true
    },
    "rules": {
        "semi": [
            "error",
            "always"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "indent": [
            "error",
            4,
            {
                "SwitchCase": 1
            }
        ],
        "eqeqeq": "off",
        "arrow-parens": [
            "error",
            "always"
        ],
        "padded-blocks": [
            "off"
        ],
        "prefer-object-spread": [
            "off"
        ],
        "object-curly-newline": [
            "off"
        ],
        "no-underscore-dangle": [
            "off"
        ],
        "func-names": [
            "error",
            "never"
        ],
        "consistent-return": [
            "off"
        ],
        "no-restricted-syntax": [
            "off"
        ],
        "guard-for-in": [
            "off"
        ],
        "prefer-destructuring": [
            "error",
            {
                "array": false,
                "object": false
            },
            {
                "enforceForRenamedProperties": false
            }
        ],
        "import/no-extraneous-dependencies": [
            "off"
        ],
        "class-methods-use-this": [
            "off"
        ],
        "linebreak-style": [
            "off"
        ],
        "no-continue": [
            "off"
        ],
        "no-await-in-loop": [
            "off"
        ],
        "no-case-declarations": [
            "off"
        ],
        "default-case": [
            "off"
        ],
        "arrow-body-style": [
            "off"
        ],
        "no-param-reassign": [
            "off"
        ],
        "no-use-before-define": [
            "off"
        ],
        "max-len": [
            "off"
        ],
        "no-shadow": [
            "off"
        ],
        "global-require": [
            "off"
        ],
        "no-cond-assign": [
            "off"
        ],
        "no-restricted-globals": [
            "off"
        ],
        "no-unused-vars": [
            "error",
            {
                "argsIgnorePattern": "^_"
            }
        ],
        "no-constant-condition": [
            "off"
        ],
        "no-trailing-spaces": [
            "error",
            {
                "ignoreComments": true
            }
        ],
        "comma-dangle": [
            "error",
            "always-multiline"
        ],
        "no-console": "warn"
    }
}